@{
}
<h1>ITORG</h1>
@{
    List<string> HYDorgs = new List<string>()
{
        "Microsoft www.microsoft.com",
        "CTS www.cognizant.com",
        "Google",
        "Amazon",
        "TCS"
    };

}

@foreach (var item in HYDorgs)
{
    if (item == "Amazon")
    {
        <div>
            <span style="font-weight:bold">@item</span>
        </div>
    }
    else
    {
        <div>
            <span>@item</span>
        </div>
    }

}